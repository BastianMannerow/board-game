<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="640.0" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="btnCounterUp" mnemonicParsing="false" prefWidth="100.0" text="+" />
      <Label fx:id="lblCounter" text="Counter: 0" />
      <Button fx:id="btnCounterDown" mnemonicParsing="false" prefWidth="100.0" text="-" />
   </children>
</VBox>
